<resources>

    <!-- Name of the skin to be shown inside the Kustom app -->
    <string name="kustom_pack_title">@string/app_name</string>

    <!-- A brief description of the pack contents -->
    <string name="kustom_pack_description">Cool Kustom komponents, wallpapers and widgets.</string>

    <!-- Set this to true if you are going to include wallpapers for kustom. false if not -->
    <bool name="includes_kustom_wallpapers">true</bool>

    <!-- Set this to true if you are going to include widgets for kustom. false if not -->
    <bool name="includes_kustom_widgets">true</bool>

    <!-- Set this to true if your assets require Kolorette to work properly. False if not -->
    <bool name="kustom_requires_kolorette">true</bool>

</resources>